generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model commits {
  id      Int     @id(map: "PK__commits__3213E83F1EB2D977") @default(autoincrement())
  repoId  Int?
  message String? @db.VarChar(255)
  repos   repos?  @relation(fields: [repoId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_commits_repos")
}

model repos {
  id       Int       @id(map: "PK__repos__3213E83FD3BF7094") @default(autoincrement())
  name     String?   @db.VarChar(255)
  authorId Int
  commits  commits[]
  users    users     @relation(fields: [authorId], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "fk_repos_users")
}

model users {
  id       Int     @id(map: "PK__users__3213E83F015F4238") @default(autoincrement())
  username String  @db.VarChar(16)
  email    String? @db.VarChar(255)
  password String  @db.VarChar(32)
  role     String  @db.VarChar(5)
  repos    repos[]
}
